{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "comma-dangle": [1, "always-multiline"],
    "no-unused-vars": [1, {"vars": "all", "args": "after-used"}],
    "quotes": [1, "double"],
    "semi": [1, "never"],
    "space-before-function-paren": [1, "always"]
  }
}
